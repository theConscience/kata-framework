.position-static
    position static !important

.is-hidden
    display none !important

.is-invisible
    opacity 0 !important
    transition opacity 0s ease !important

.no-padding
    padding 0 !important

.no-padding-left
    padding-left 0 !important

.no-padding-right
    padding-right 0 !important

.no-margin
    margin 0 !important

.no-margin-left
    margin-left 0 !important

.no-margin-right
    margin-right 0 !important
